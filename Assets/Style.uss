}

.text {
    height: 50px;
    width: 186px;
    flex-direction: row;
    -unity-text-align: upper-center;
    top: auto;
    font-size: 24px;
    margin-left: 10px;
    margin-right: 10px;
    margin-top: 10px;
    margin-bottom: 10px;
}
